@model IEnumerable<string>

<div class="btn-group btn-group-sm btn-group-justified">
    @Html.ActionLink("Home", "ListAll", "Book", null, new { @class = "btn btn-default btn-sm" })

    @foreach (var link in Model)
    {
        @Html.RouteLink(link, new { controller = "Book", action = "List", specilization = link, pageno = 1 },
    new { @class = "btn btn-block btn-default btn-lg " + ((link == ViewBag.SelectedSpec) ? "btn-primary" : "").ToString() }
    )
    }
</div>


